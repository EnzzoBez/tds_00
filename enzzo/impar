#include <stdio.h>

int main() {
    int numeros[10];
    int impares = 0;

    // Leitura de 10 números
    printf("Digite 10 numeros:\n");
    for (int i = 0; i < 10; ++i) {
        printf("Numero %d: ", i + 1);
        scanf("%d", &numeros[i]);
    }

    // Verificação e contagem de números ímpares
    for (int i = 0; i < 10; ++i) {
        if (numeros[i] % 2 != 0) {
            printf("%d eh impar.\n", numeros[i]);
            impares++;
        }
    }

    // Mostrar o total de números ímpares
    printf("Total de numeros impares: %d\n", impares);

    return 0;
}
